{
  "name": "@viewfly/core",
  "version": "0.0.20",
  "description": "Viewfly is a simple and easy-to-use JavaScript framework with an intuitive development experience.",
  "main": "./bundles/index.js",
  "module": "./bundles/index.esm.js",
  "typings": "./bundles/public-api.d.ts",
  "exports": {
    ".": {
      "types": "./bundles/public-api.d.ts",
      "import": "./bundles/index.esm.js",
      "require": "./bundles/index.js"
    },
    "./jsx-runtime": {
      "types": "./jsx-runtime/index.d.ts",
      "import": "./jsx-runtime/index.esm.js",
      "require": "./jsx-runtime/index.js"
    },
    "./jsx-dev-runtime": {
      "types": "./jsx-runtime/index.d.ts",
      "import": "./jsx-runtime/index.esm.js",
      "require": "./jsx-runtime/index.js"
    }
  },
  "scripts": {
    "build:lib": "rimraf bundles && rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "publish:lib": "npm run build:lib && npm publish --access=public"
  },
  "license": "MIT",
  "keywords": [],
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^3.26.3",
    "tslib": "^2.6.0"
  },
  "author": {
    "name": "Tanbo",
    "email": "tanbohb@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viewfly/viewfly.git"
  },
  "bugs": {
    "url": "https://github.com/viewfly/viewfly.git/issues"
  }
}
